{
    "manifest_version": 2,
    "name": "fillmore",
    "version": "0.0.1",
    "browser_action": {
        "default_popup": "index.html"
    },
    "content_scripts": [{
        "matches": ["http://*/*", "https://*/*"],
        "js": [
            "content.js"
        ],
        "run_at": "document_end"
    }]
}